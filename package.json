{
  "name": "brandonb-sites",
  "private": true,
  "type": "module",
  "dependencies": {
    "@aws-sdk/credential-provider-ini": "^3.272.0",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "broken-link-checker": "^0.7.8",
    "browser-sync": "^2.27.11",
    "canvas": "^2.11.0",
    "chalk": "^5.2.0",
    "del": "^7.0.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.34.0",
    "flickr-sdk": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-awspublish": "^7.1.0",
    "gulp-babel": "^8.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-debug": "^4.0.0",
    "gulp-duration": "^0.0.0",
    "gulp-gm": "^0.0.9",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "8.0.0",
    "gulp-inline-source": "^4.0.0",
    "gulp-less": "^5.0.0",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-size": "^4.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "jsdom": "^21.1.0",
    "lodash.merge": "^4.6.2",
    "mkdirp": "^2.1.3",
    "ngrok": "^4.3.3",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "surge": "^0.24.0-rc.15",
    "sync-request": "^6.1.0",
    "yargs": "^17.7.0"
  },
  "scripts": {
    "deploy:dryrun": "cd brandonb.ca && JEKYLL_ENV=production gulp deploy_dryrun",
    "deploy": "cd brandonb.ca && JEKYLL_ENV=production gulp deploy",
    "lint": "cd brandonb.ca && eslint '_assets/scripts/**/*.js'",
    "prettier": "cd brandonb.ca && prettier '_assets/scripts/**/*.js' --write",
    "start:dev": "cd brandonb.ca && gulp --noserviceworker",
    "start": "cd brandonb.ca && gulp",
    "surge": "cd brandonb.ca && ./node_modules/.bin/surge",
    "task:flickr": "cd brandonb.ca && node ./gulp_tasks/flickr.js",
    "task:generate-share-images": "cd brandonb.ca && gulp generate_share_images"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "browserslist": [
    "defaults"
  ],
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2022,
      "sourceType": "module"
    }
  }
}
